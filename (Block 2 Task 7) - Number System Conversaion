#include <stdio.h>
#include <string.h>

int pow_(int n, int p){
    int new_n = 1;
    for (int i = 0; i < p; i++) { new_n *= n; }
    return new_n;
}
void numberSystemConversion(int p, char num[100], int q){
    char alph[38] = "0123456789abcdefghijklmnopqrstuvwxyz";
    int dec = 0;
    
    int current_char_index = 0;
    for (int i = strlen(num)-1; i >= 0 ; i--) { 
        for (int j = 0; j < p; j++){
            if (num[i] == alph[j]) {current_char_index = j; break;}
        }
        dec += current_char_index * (pow_(p, strlen(num)-1-i));
    }
    if (dec == 0) { printf("0"); return;}
    char str[1000000] = "";
    int c_i = 0;
    while (dec != 0){
        str[c_i] = alph[dec % q];
        c_i++;
        dec /= q;
    }
    for (int i = c_i-1; i >= 0; i--) {
    printf("%c", str[i]);
}
}

int main(void){
    int p, q;
    char num[100000];
    scanf("%d %d %s", &p, &q, num); 
    numberSystemConversion(p, num, q);
    return 0;
}

