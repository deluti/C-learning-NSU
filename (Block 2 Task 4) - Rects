#include <stdio.h>
int canPlace1(int x1, int y1, int x2, int y2){
    if ((x1 >= x2 && y1 >= y2)) {return 1;}
    return 0;
}
int canPlace2(int x1, int y1, int x2, int y2){
    if ((x1 >= y2 && y1 >= x2)) {return 1;}
    return 0;
}
int main(void){ 
    //3 2 2 1 1 3
    int a1, b1, a2, b2, a3, b3;
    scanf("%d %d %d %d %d %d", &a1, &b1, &a2, &b2, &a3, &b3);
    if (canPlace1(a1, b1, a2, b2)){
        if ((a1 - a2) * b1 >= a3 * b3 && a1 - a2 > 0 && (canPlace1(a1 - a2, b1, a3, b3) || canPlace2(a1 - a2, b1, a3, b3))) { printf("YES"); return 0; };
        if ((b1 - b2) * a1 >= a3 * b3 && b1 - b2 > 0 && (canPlace1(a1, b1 - b2, a3, b3) || canPlace2(a1, b1 - b2, a3, b3))) { printf("YES"); return 0; };
    }
    if (canPlace2(a1, b1, a2, b2)){
        if ((a1 - b2) * b1 >= a3 * b3 && a1 - b2 > 0 && (canPlace2(a1 - b2, b1, a3, b3) || canPlace1(a1 - b2, b1, a3, b3))) { printf("YES"); return 0; };
        if ((b1 - a2) * a1 >= a3 * b3 && b1 - a2 > 0 && (canPlace2(a1, b1 - a2, a3, b3) || canPlace1(a1, b1 - a2, a3, b3))) { printf("YES"); return 0; };
    }
    printf("NO");
    return 0; 
}
