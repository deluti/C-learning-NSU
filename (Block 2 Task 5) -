#include <stdio.h>

int pow_(int n, int p){
    int new_n = 1;
    for (int i = 0; i < p; i++) { new_n *= n; }
    return new_n;
}

int main(void){
    long long num_of = 0;
    scanf("%lld", &num_of);
    char bins[num_of];
    scanf("%s", bins);
    for (int i = 0; i < num_of; i+=8){
        int current_byte = 0;
        for (int j = i; j < i+8; j++){
            if (bins[j] == '1'){
                current_byte += 1 * pow_(2, j - i);
            }
        }
        printf("%d ", current_byte);
    }
    return 0;
}
